package fahrenheit;

import java.util.Scanner;


public class Fahrenheit {

    
    public static void main(String[] args) {
        Scanner grava = new Scanner(System.in);
        
        double C;
        System.out.println("Digite O grau a ser convertido ");
        C = grava.nextDouble();
        
        double F = ((C * 9) / 5) + 32;
        
        System.out.println("O graus celsius convertido para fahrenheit fica: " + F);
    }
    
}
